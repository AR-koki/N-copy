<div class="container" style="background-color: black;width: 100%;padding: 0 0 100px 100px;height: 100vh;">
	<%= render 'users/users/header' %>
	<div>
		<% @mylists.each do |my_list| %>
			<%= link_to users_movie_path(my_list.movie) do %>
				<div style="width: 200px;height: 120px;border: 1px solid;float: left;margin-right: 3px;border-radius: 5px;">
					<%= my_list.movie.title %>
				</div>
			<% end %>
		<% end %>
	</div>
</div>